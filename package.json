{
  "name": "nextjs-freelance-website",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "rimraf .next && npm run lint && npm run format:fix && npm run test && next build",
    "start": "next start",
    "lint": "next lint && stylelint **/*.scss --fix -f verbose",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "build-static": "rimraf .next && rimraf out  && next lint && next build && next export",
    "test": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006 -s ./src",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
